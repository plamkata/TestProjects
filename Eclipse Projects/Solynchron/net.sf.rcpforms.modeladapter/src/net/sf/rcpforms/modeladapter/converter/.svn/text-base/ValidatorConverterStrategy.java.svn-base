package net.sf.rcpforms.modeladapter.converter;

import org.eclipse.core.databinding.conversion.IConverter;
import org.eclipse.core.databinding.validation.IValidator;

public class ValidatorConverterStrategy implements IValidatorConverterStrategy
{
    IConverter modelToTargetConverter;
    IConverter targetToModelConverter;
    IValidator modelToTargetValidator;
    IValidator targetToModelValidator;
    
    public IConverter getModelToTargetConverter()
    {
        return modelToTargetConverter;
    }

    public IValidator getModelToTargetValidator()
    {
        return modelToTargetValidator;
    }

    public IConverter getTargetToModelConverter()
    {
        return targetToModelConverter;
    }

    public IValidator getTargetToModelValidator()
    {
        return targetToModelValidator;
    }

    public void setModelToTargetConverter(IConverter modelToTargetConverter)
    {
        this.modelToTargetConverter = modelToTargetConverter;
    }

    public void setTargetToModelConverter(IConverter targetToModelConverter)
    {
        this.targetToModelConverter = targetToModelConverter;
    }

    public void setModelToTargetValidator(IValidator modelToTargetValidator)
    {
        this.modelToTargetValidator = modelToTargetValidator;
    }

    public void setTargetToModelValidator(IValidator targetToModelValidator)
    {
        this.targetToModelValidator = targetToModelValidator;
    }

}
