<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="net.sf.rcpforms.examples.emf.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="net.sf.rcpforms.examples.emf.Perspective"
            id="net.sf.rcpforms.examples.emf.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="View"
            class="net.sf.rcpforms.examples.emf.View"
            id="net.sf.rcpforms.examples.emf.view">
      </view>
   </extension>

</plugin>
