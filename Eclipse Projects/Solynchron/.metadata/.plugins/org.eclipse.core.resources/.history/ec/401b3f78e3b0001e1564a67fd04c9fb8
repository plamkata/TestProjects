package com.damnhandy.aspects.bean.impl;

import java.beans.IndexedPropertyChangeEvent;

/**
 * @author plamKaTa
 *
 */
public class NestedIndexedPropertyChangeEvent extends
		IndexedPropertyChangeEvent {

	/**
	 * @param source
	 * @param propertyName
	 * @param oldValue
	 * @param newValue
	 * @param index
	 */
	public NestedIndexedPropertyChangeEvent(Object source, String propertyName,
			Object oldValue, Object newValue, int index) {
		super(source, propertyName, oldValue, newValue, index);
		// TODO Auto-generated constructor stub
	}

}
